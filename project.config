[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = Public wiki with useful information about Gerrit Code Review
[access "refs/*"]
	owner = group user/GerritForge Ltd (gerritforge-ltd)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/GerritForge Ltd (gerritforge-ltd)
	label-Verified = -1..+1 group user/GerritForge Ltd (gerritforge-ltd)
	push = group user/GerritForge Ltd (gerritforge-ltd)
	read = group user/GerritForge Ltd (gerritforge-ltd)
	rebase = group user/GerritForge Ltd (gerritforge-ltd)
	removeReviewer = group user/GerritForge Ltd (gerritforge-ltd)
	submit = group user/GerritForge Ltd (gerritforge-ltd)
[access "refs/heads/*"]
	create = group user/GerritForge Ltd (gerritforge-ltd)
	push = +force group user/GerritForge Ltd (gerritforge-ltd)
	pushMerge = group user/GerritForge Ltd (gerritforge-ltd)
	read = group user/GerritForge Ltd (gerritforge-ltd)
[access "refs/tags/*"]
	push = group user/GerritForge Ltd (gerritforge-ltd)
	push = +force group user/GerritForge Ltd (gerritforge-ltd)
